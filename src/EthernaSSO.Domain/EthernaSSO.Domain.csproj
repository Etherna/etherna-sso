<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>Etherna.SSOServer.Domain</RootNamespace>

    <Authors>Etherna Sagl</Authors>
	  
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <Nullable>enable</Nullable>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
    <AnalysisMode>AllEnabledByDefault</AnalysisMode>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DomainEvents" Version="1.1.1" />
    <PackageReference Include="MongODM.Core" Version="0.21.0" />
    <PackageReference Include="Nethereum.Accounts" Version="3.8.0" />
  </ItemGroup>

</Project>
